! A NOnonsens Xresources file with just some XTerm configs and Tango palette,
! I'm trying to keep it as short as possible.




! Xterm config

Xft.dpi: 100
Xft.antialias: true
Xft.hinting: true
Xft.rgba: rgb
Xft.autohint: true
Xft.hintstyle: hintfull
Xft.lcdfilter: lcdfilter

XTerm*renderFont: true
XTerm*faceName: xft:mononoki Nerd Font
XTerm*faceSize: 8.5
XTerm*utf8: 2
XTerm*locale: true
XTerm*loginshell: true
XTerm*savelines: 16384
XTerm*charClass: 33:48,36-47:48,58-59:48,61:48,63-64:48,95:48,126:48
XTerm*background: #282A36
XTerm*foreground: #F8F8F2
XTerm*fullscreen: never

XTerm.vt100.metaSendsEscape: true
XTerm.vt100.backarrowKey: false
XTerm.ttyModes: erase ^?
XTerm.omitTranslation: fullscreen
XTerm.vt100.translations: #override \n\
    Ctrl Alt <Key> -: smaller-vt-font() \n\
    Ctrl Alt <Key> +: larger-vt-font()

! XTerm*cursorBlink: on
! XTerm*cursorOnTime: 400
! XTerm*cursorOffTime: 250
! XTerm*cursorColor: #F1FA8C




! UXterm config

UXTerm*renderFont: true
UXTerm*faceName: xft:mononoki Nerd Font
UXTerm*faceSize: 8.5
UXTerm*utf8: 2
UXTerm*locale: true
UXTerm*loginshell: true
UXTerm*savelines: 16384
UXTerm*charClass: 33:48,36-47:48,58-59:48,61:48,63-64:48,95:48,126:48
UXTerm*background: #282A36
UXTerm*foreground: #F8F8F2
UXTerm*fullscreen: never

UXTerm.vt100.metaSendsEscape: true
UXTerm.vt100.backarrowKey: false
UXTerm.ttyModes: erase ^?
UXTerm.omitTranslation: fullscreen
UXTerm.vt100.translations: #override \n\
    Ctrl Alt <Key> j: smaller-vt-font() \n\
    Ctrl Alt <Key> k: larger-vt-font()




! Rofi config

! rofi.color-normal: #282A36, #BFBFBF, #282A36, #BFBFBF, #282A36
! rofi.color-window: #282A36, #BFBFBF
! rofi.bw: 2
! rofi.case-sensitive: false
! rofi.fuzzy: true
! rofi.line-margin: 2
! rofi.separator-style: none
! rofi.hide-scrollbar: true




